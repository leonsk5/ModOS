import os    # importa do modulo 

curDir = os.getcwd()  # verificar o diretorio atual do susuario
 
print(curDir)       

os.mkdir('newDir') # cria o diretorio

import time      # importa o modulo time

time.sleep(2)  # usando o modulo time para gerar 2 segundos de delay

os.rename('newDir', 'newDir2') # renomeia o diretorio
time.sleep(2)

os.rmdir('newDir2') # deleta o diretorio
